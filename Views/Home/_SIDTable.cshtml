@model SchoolZone.SID.Mvc.Models.SIDTableModel
@using System.Data

<div class="dt-rowReorder-noOverflow">
    <div class="table-responsive">
        <table class="table table-striped table-bordered display data-table" cellspacing="0">
            <thead>
                <tr>
                    @foreach (DataColumn col in Model.Table.Columns)
                    {
                        <th>@col.ColumnName</th>
                    }
                </tr>
            </thead>
            <tbody>
                @foreach (DataRow row in Model.Table.Rows)
            {
                    <tr>
                        @foreach (DataColumn col in Model.Table.Columns)
                        {
                            <td>@row[col.ColumnName]</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>